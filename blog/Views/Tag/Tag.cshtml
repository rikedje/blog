@model IEnumerable<EdjeBlog.Models.BlogContent>


@{

    ViewBag.Title = "[ Rikard Edje Blog | Tag ]";    

    foreach (var item in Model) {
        if(item.GetType() == typeof(EdjeBlog.Models.Post))
        {
            Html.RenderPartial("_PostDetails", item);
        } 
        else if(item.GetType() == typeof(EdjeBlog.Models.Page))
        {
            Html.RenderPartial("_PageDetails", item);
        }
    }
}